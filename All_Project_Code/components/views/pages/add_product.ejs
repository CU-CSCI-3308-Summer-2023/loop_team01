<%- include ('../partials/header') %> 
<%- include ('../partials/secondary_menu') %>

<html>
    <body>
        <form action='/add' method='POST' id="fileInput">
            <div class="form-group">
              <label for="exampleFormControlFile1">Item image</label>
              <input type="text" class="form-control" id="exampleFormControlFile1" name="image_url">
            </div>
            <div class="form-group">
                <label for="name">Item Name</label>
                <input type="ItemName" class="form-control" id="ItemName" name='name' aria-describedby="ItemName" placeholder="Shirt...">
            </div>
            <div class="form-group">
                <label for="itemDescription">Item Description</label>
                <input type="Item Description" class="form-control" name='description' id="itemDescription" placeholder="Item Description">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </body>
</html>

